<!DOCTYPE html>
<html>
  <head>
    <title>Admin Page</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/ejs/3.1.6/ejs.min.js"></script>
  </head>
  <body>
    <h1>Admin Page</h1>
    <table>
      <thead>
        <tr>
          <th>User ID</th>
          <th>Name</th>
          <th>Role</th>
          <th>Password</th>
        </tr>
      </thead>
      <tbody>
        <% users.forEach(function(user) { %>
        <tr>
          <td><%= user.userID %></td>
          <td><%= user.name %></td>
          <td><%= user.role %></td>
          <td><%= user.password %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
